    "nums = [1,2,3,4,5,6]\n",
    "def inNguoc(nums):\n",
    "    for i in range(len(nums)//2):\n",
    "        nums[i], nums[len(nums) - i - 1] = nums[len(nums) - i - 1], nums[i]\n",
    "    return nums\n",
    "        \n",
    "\n",
    "inNguoc(nums)\n",
    "      "
   ]
